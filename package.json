{
  "name": "redux-thunker",
  "version": "0.0.1",
  "description": "Redefined redux-thunk",
  "main": "lib/index.js",
  "repository": "https://github.com/march08/redux-thunker.git",
  "author": "Roman Nguyen <march08@outlook.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx ./src",
    "build": "yarn build:commonjs && yarn build:umd && yarn build:umd:min",
    "clean": "rimraf lib dist es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack",
    "build:umd:min":
      "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack --optimize-minimize"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
